{
    "name": "uproot",
    "version": "1.0.0",
    "description": "Decentralized application for education and funding",
    "main": "index.js",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build && gulp licenses",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
        "compile-sass": "node-sass src/assets/scss/blk-design-system-react.scss src/assets/css/blk-design-system-react.css",
        "minify-sass": "node-sass src/assets/scss/blk-design-system-react.scss src/assets/css/blk-design-system-react.min.css --output-style compressed",
        "map-sass": "node-sass src/assets/scss/blk-design-system-react.scss src/assets/css/blk-design-system-react.css --source-map true",
        "solhint": "./node_modules/.bin/solhint -f table contracts/**/*.sol",
        "docgen": "solidoc ./ ./docs"
    },
    "keywords": [
        "react",
        "reactjs",
        "blk",
        "blk-react",
        "design",
        "design-react",
        "system",
        "system-react",
        "design-system",
        "design-system-react",
        "blk-design",
        "blk-design-react",
        "blk-system",
        "blk-system-react",
        "blk-design-system",
        "blk-design-system-react"
    ],
    "dependencies": {
        "@0x/contracts-utils": "^4.4.3",
        "@chainlink/contracts": "^0.0.3",
        "@opengsn/gsn": "^0.8.4",
        "@openzeppelin/contracts": "3.0.1",
        "@openzeppelin/upgrades": "2.8.0",
        "@uniswap/sdk": "^2.0.1",
        "@uniswap/v2-periphery": "^1.0.0-beta.0",
        "chart.js": "2.9.3",
        "classnames": "2.2.6",
        "gulp": "4.0.2",
        "gulp-append-prepend": "1.0.8",
        "moment": "2.24.0",
        "node-sass": "4.13.1",
        "nouislider": "14.2.0",
        "perfect-scrollbar": "1.5.0",
        "react": "16.13.1",
        "react-bootstrap-switch": "15.5.3",
        "react-chartjs-2": "2.9.0",
        "react-datetime": "2.16.3",
        "react-dom": "16.13.1",
        "react-router-dom": "5.1.2",
        "react-scripts": "3.4.1",
        "reactstrap": "8.4.1",
        "rimble-ui": "^0.14.0",
        "solhint": "^2.3.1",
        "solidoc": "^1.0.5",
        "styled-components": "^5.1.0"
    },
    "devDependencies": {
        "@0x/sol-trace": "^3.0.8",
        "@0x/web3-wrapper": "^7.0.7",
        "@openzeppelin/cli": "2.8.2",
        "apollo-fetch": "^0.7.0",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "chalk": "4.0.0",
        "eth-gas-reporter": "0.2.17",
        "ethers": "^4.0.47",
        "ganache-cli": "6.9.1",
        "husky": "^4.2.5",
        "mkdirp": "^1.0.4",
        "mocha-junit-reporter": "1.23.3",
        "oz-console": "2.0.1",
        "shelljs": "0.8.4",
        "solc": "0.6.6",
        "solidity-coverage": "0.7.5",
        "truffle": "5.1.24",
        "truffle-deploy-registry": "0.5.1",
        "truffle-hdwallet-provider": "1.0.17",
        "zos-truffle-merge": "0.0.5"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "optionalDependencies": {
        "eslint-plugin-flowtype": "3.13.0",
        "typescript": "3.8.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MatheusDaros/Uproot.git"
    },
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/MatheusDaros/Uproot/issues"
    }
}
